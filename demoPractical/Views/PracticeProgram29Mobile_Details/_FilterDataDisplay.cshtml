@model IEnumerable<demoPractical.Mobile_Details>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ModelName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModelPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModelImage)
        </th>
        <th></th>
    </tr>

@if (Model != null) { 
foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ModelName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModelPrice)
        </td>
        <td>
            <img src="@Url.Content("~/Content/Images/"+@item.ModelImage)" height="100" width="100" />
            @*  @Html.DisplayFor(modelItem => item.ModelImage)*@
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}
}

</table>
